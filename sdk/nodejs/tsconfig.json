        "getProjectDirectory.ts",
        "index.ts",
        "project.ts",
        "projectDomain.ts",
        "projectEnvironmentVariable.ts",
        "provider.ts",
        "sharedEnvironmentVariable.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
